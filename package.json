{
  "name": "cshield",
  "version": "0.1.0",
  "description": "cShield Security for Blockchain & Ethereum smart contract audits",
  "homepage": ".",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cypress": "cypress open",
    "i18n:extract": "NODE_ENV=development lingui extract --locale en-US",
    "prei18n:extract": "touch src/locales/en-US.po",
    "deploy": "vercel --prod --force",
    "docker:nginx": "docker-compose -f docker-compose-nginx.yml up -d",
    "docker": "yarn telemetry:disable && yarn build && NODE_ENV=production docker-compose up -d",
    "docker:restart": "docker-compose restart",
    "docker:clear": "docker-compose down -v && docker-compose rm -sfv",
    "docker:reset": "yarn docker:clear && yarn docker",
    "docker:status": "docker-compose ps -a",
    "docker:config": "docker-compose config",
    "docker:logs": "docker logs --follow web_node"
  },
  "dependencies": {
    "@emotion/cache": "^11.4.0",
