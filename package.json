    "next": "11.1.0",
    "nprogress": "^0.2.0",
    "polished": "^4.1.3",
    "qs": "^6.10.1",
    "react": "17.0.2",
    "react-confetti": "^6.0.1",
    "react-device-detect": "^1.17.0",
    "react-dom": "17.0.2",
    "react-ga": "^3.3.0",
    "react-is": "^17.0.2",
    "react-markdown": "^7.0.0",
    "react-redux": "^7.2.4",
    "react-spring": "^9.2.4",
    "react-use-gesture": "^9.1.3",
